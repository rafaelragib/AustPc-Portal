<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.aust.austpc.austpcbeta6">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="AUSTPC Portal"
        android:roundIcon="@drawable/ic_app_launcher"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity
            android:name=".HomeActivity"
            android:label="AUSTPC Portal"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity android:name=".blog.BlogActivity" />
        <activity android:name=".auth.LoginActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".auth.SignupActivity" />
        <activity android:name=".database.ProfileSetupActivity" />
        <activity android:name=".Event.EventCreateActivity" />
        <activity android:name=".Event.EventUpdateDelete"></activity>
    </application>

</manifest>